#tag Events PushButton1
	#tag Event
		Sub Action()
		  dim dlg as new OpenDialog
		  if dlg.ShowModal = nil then
		    return
		  end if
		  
		  dim qt_movie as QTMovie = QTMovie.LoadFromFolderItem(dlg.Result)
		  if qt_movie <> nil then
		    QTMovieView1.Movie = qt_movie
		  end if
		  
		  
		End Sub
	#tag EndEvent
