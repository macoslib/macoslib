	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  myGradient3.drawInRectRelativeCenterPosition (Cocoa.NSMakeRect (0, 0, self.Width, self.Height), _
		  cocoa.NSMakePoint (1.0,-0.5))
		  
		  
		End Sub
