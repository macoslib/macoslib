#tag WindowCode
	#tag Method, Flags = &h21
		Private Sub hasFocus(ctrl as RectControl, acquired as Boolean)
		  if acquired then
		    currEventFocus = ctrl
		  else
		    if currEventFocus = ctrl then
		      currEventFocus = nil
		    end
		  end
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h21
		Private currEventFocus As RectControl
	#tag EndProperty


#tag EndWindowCode

#tag Events date1
	#tag Event
		Sub GotFocus()
		  hasFocus me, true
		End Sub
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  hasFocus me, false
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events edit2
