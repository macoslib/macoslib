#tag Events Timer1
	#tag Event
		Sub Action()
		  // check periodically which control has the focus
		  
		  dim c as RectControl
		  dim s as String
		  
		  c = self.Focus
		  if c <> nil then
		    s = c.Name
		  end
		  if focusWinTxt.Text <> s then
		    focusWinTxt.Text = s
		  end if
		  
		  s = ""
		  c = currEventFocus
		  if c <> nil then
		    s = c.Name
		  end
		  if focusEventTxt.Text <> s then
		    focusEventTxt.Text = s
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
